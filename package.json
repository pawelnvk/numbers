{
  "name": "numbers",
  "private": true,
  "version": "0.0.1",
  "description": "Numbers App",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "^0.10.0",
    "@stencil/sass": "^0.0.5"
  },
  "devDependencies": {
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawelnvk/numbers.git"
  },
  "author": "Pawe≈Ç Nowak",
  "license": "MIT",
  "homepage": "https://github.com/pawelnvk/numbers",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
